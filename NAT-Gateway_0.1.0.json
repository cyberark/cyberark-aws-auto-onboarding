{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "PublicNATSubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "CidrBlock": {
                    "Ref": "PublicNATGWSubnetCIDR"
                },
                "AvailabilityZone": {
                    "Fn::Select": [
                        "1",
                        {
                            "Fn::GetAZs": {
                                "Ref": "AWS::Region"
                            }
                        }
                    ]
                },
                "VpcId": {
                    "Ref": "ComponentsVpc"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Public NAT"
                    }
                ]
            }
        },
		"PrivateNATSubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "CidrBlock": {
                    "Ref": "PrivateNATGWSubnetCIDR"
                },
                "AvailabilityZone": {
                    "Fn::Select": [
                        "1",
                        {
                            "Fn::GetAZs": {
                                "Ref": "AWS::Region"
                            }
                        }
                    ]
                },
                "VpcId": {
                    "Ref": "ComponentsVpc"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Private NAT"
                    }
                ]
            }
        },
		"PublicNATRT": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "ComponentsVpc"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Public NAT RT"
                    }
                ]
            }
        },
		"PrivateNATRT": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "ComponentsVpc"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Private NAT RT"
                    }
                ]
            }
        },
		"PublicNATEIP": {
            "Type": "AWS::EC2::EIP",
            "Properties": {
                "Domain": "vpc"
            }
        },
		"PublicNATGW": {
            "Type": "AWS::EC2::NatGateway",
            "Properties": {
                "AllocationId": {
                    "Fn::GetAtt": [
                        "PublicNATEIP",
                        "AllocationId"
                    ]
                },
                "SubnetId": {
                    "Ref": "PublicNATSubnet"
                }
            }
        },
		"PublicNATRTAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PublicNATRT"
                },
                "SubnetId": {
                    "Ref": "PublicNATSubnet"
                }
            }
        },
		"PrivateNATRTAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PrivateNATRT"
                },
                "SubnetId": {
                    "Ref": "PrivateNATSubnet"
                }
            }
        },
		"PublicNATtoIGWRoute": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "RouteTableId": {
                    "Ref": "PublicNATRT"
                },
                "GatewayId": {
                    "Ref": "CurrentIGWInComponentsVPC"
                }
            }
        },
		"PrivateNATtoNatRoute": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "RouteTableId": {
                    "Ref": "PrivateNATRT"
                },
                "NatGatewayId": {
                    "Ref": "PublicNATGW"
                }
            }
        },
		"NATNACL": {
            "Type": "AWS::EC2::NetworkAcl",
            "Properties": {
                "VpcId": {
                    "Ref": "ComponentsVpc"
                }
            }
        },
		"PublicNATAclEntry1": {
            "Type": "AWS::EC2::NetworkAclEntry",
            "Properties": {
                "CidrBlock": "0.0.0.0/0",
                "Egress": "true",
                "Protocol": "-1",
                "RuleAction": "allow",
                "RuleNumber": "100",
                "NetworkAclId": {
                    "Ref": "NATNACL"
                }
            }
        },
        "PublicNATAclEntry2": {
            "Type": "AWS::EC2::NetworkAclEntry",
            "Properties": {
                "CidrBlock": "0.0.0.0/0",
                "Protocol": "-1",
                "RuleAction": "allow",
                "RuleNumber": "100",
                "NetworkAclId": {
                    "Ref": "NATNACL"
                }
            }
        },
		"PrivateNATAclEntry1": {
            "Type": "AWS::EC2::NetworkAclEntry",
            "Properties": {
                "CidrBlock": "0.0.0.0/0",
                "Egress": "true",
                "Protocol": "-1",
                "RuleAction": "allow",
                "RuleNumber": "100",
                "NetworkAclId": {
                    "Ref": "NATNACL"
                }
            }
        },
        "PrivateNATAclEntry2": {
            "Type": "AWS::EC2::NetworkAclEntry",
            "Properties": {
                "CidrBlock": "0.0.0.0/0",
                "Protocol": "-1",
                "RuleAction": "allow",
                "RuleNumber": "100",
                "NetworkAclId": {
                    "Ref": "NATNACL"
                }
            }
        },
		"PublicNATNACLAssociation": {
            "Type": "AWS::EC2::SubnetNetworkAclAssociation",
            "Properties": {
                "NetworkAclId": {
                    "Ref": "NATNACL"
                },
                "SubnetId": {
                    "Ref": "PublicNATSubnet"
                }
            }
        },
		"PrivateNATNACLAssociation": {
            "Type": "AWS::EC2::SubnetNetworkAclAssociation",
            "Properties": {
                "NetworkAclId": {
                    "Ref": "NATNACL"
                },
                "SubnetId": {
                    "Ref": "PrivateNATSubnet"
                }
            }
        }
    },
    "Description": "",
    "Parameters": {
        "PublicNATGWSubnetCIDR": {
            "Description": "IPv4 address range for the Public NAT Gateway subnet",
            "Type": "String",
            "MinLength": "9",
            "MaxLength": "18",
            "Default": "10.10.0.0/28",
            "AllowedPattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(\\/(1[6-9]|2[0-8]))$",
            "ConstraintDescription": "Must be a valid IP CIDR range of the form x.x.x.x/x."
        },
		"PrivateNATGWSubnetCIDR": {
            "Description": "IPv4 address range for the Private NAT Gateway subnet",
            "Type": "String",
            "MinLength": "9",
            "MaxLength": "18",
            "Default": "10.10.10.0/28",
            "AllowedPattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(\\/(1[6-9]|2[0-8]))$",
            "ConstraintDescription": "Must be a valid IP CIDR range of the form x.x.x.x/x."
        },
        "ComponentsVpc": {
            "Description": "The Vpc Id of the Components",
            "Type": "AWS::EC2::VPC::Id"
        },
        "CurrentIGWInComponentsVPC": {
            "Type": "String",
			"MinLength": "1",
			"Default": "igw-c25963ab"
        }

    },
    "Conditions": {},
    "Metadata": {},
    "Mappings": {},
	"Outputs" : {
		"PrivateNATSubnetOutput" : {
			"Description" : "The Private NAT subnet Id created",
			"Value" : {"Ref": "PrivateNATSubnet"}
		}
	}
}